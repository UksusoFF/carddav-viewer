{
    "name": "uksusoff/carddav-viewer",
    "description": "CardDAV Viewer",
    "license": "MIT",
    "keywords": [],
    "type": "project",
    "require": {
        "php": ">=7.1",
        "ext-xmlwriter": "*",
        "jeroendesloovere/vcard": "^1.7",
        "rappasoft/laravel-helpers": "^1.0",
        "sabre/dav": "~3.2.0",
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.3",
        "symfony/dotenv": "^4.0",
        "tuupola/slim-basic-auth": "^3.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "cs:fix": [
            "vendor/bin/php-cs-fixer fix . --config=.php_cs --allow-risky=yes"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
